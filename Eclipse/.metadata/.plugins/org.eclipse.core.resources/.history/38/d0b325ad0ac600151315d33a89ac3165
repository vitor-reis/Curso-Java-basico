package com.vitor.cursojava.aula11;

public class DeverDeCasa {

	public static void main(String[] args) {
		
		long haha = 865364;
		int i = 151;
		String nomeDaMinhaMae = "Eunice";
		int idade = 42;
		double meuDinheiro = 27.35;
		
		System.out.println("Alfa = " + haha );
		System.out.println("Nome da Minha Mae " +nomeDaMinhaMae);
		System.out.println("Idade da Minha Mae "+ idade +" Anos");
		System.out.println("My Money "+ meuDinheiro);
		System.out.println("Idade Menor que 18 = " + idade);
		System.out.println(i);
	
		
	}

}
