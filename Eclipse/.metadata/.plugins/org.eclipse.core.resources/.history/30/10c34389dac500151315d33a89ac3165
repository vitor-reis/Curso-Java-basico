package com.vitor.cursojava.aula11;

public class CuriosidadeDosInteiros {

	public static void main(String[] args) {
		
		int var1 = 2147483647;
				
		int var2 = 1;
		
		System.out.println(var1 + var2);

		
	// Maior Número Possivel no "Int" 2147483647
	// Menor Número Possivel no "Int" -2147483648
	}

}
